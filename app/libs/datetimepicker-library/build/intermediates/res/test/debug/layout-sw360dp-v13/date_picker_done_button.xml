<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dialog_button_touch_target"
    android:layout_marginTop="16dp"
    android:layout_marginBottom="0dp">

    <Button
        android:id="@+id/cancel"
        style="@style/DialogButton"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dialog_button_height"
        android:text="@string/cancel_label"
        android:layout_toLeftOf="@id/done"
        android:layout_toStartOf="@id/done"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"/>

    <Button
        style="@style/DialogButton"
        android:id="@id/done"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dialog_button_height"
        android:text="@string/done_label"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="@dimen/dialog_button_margin_right"
        android:layout_marginEnd="@dimen/dialog_button_margin_right"/>
</RelativeLayout><!-- From: file:/C:/Users/Marco/AndroidStudioProjects/UnofficialFunCheapSF/app/libs/datetimepicker-library/res/layout-sw360dp/date_picker_done_button.xml --><!-- From: file:/C:/Users/Marco/AndroidStudioProjects/UnofficialFunCheapSF/app/libs/datetimepicker-library/build/intermediates/bundles/debug/res/layout-sw360dp-v13/date_picker_done_button.xml -->